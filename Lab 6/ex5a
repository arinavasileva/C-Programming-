#include <iostream>
#include <cstring>

class String {
private:
    char* str;

public:
    // Constructor conversion 
    String(const char* cstr) {
        
    }

    
    operator const char*() const {
        
    }

   

};

int main() {
    String s;
    char name[30];

    // Conversion c-string -> String
    s = "Matti";
    std::cout << s;

    // Conversion String -> c-string
    strcpy(name, s);
    std::cout << name;

    return 0;
}